{
  "name": "j6t",
  "version": "1.3.1",
  "description": "a jquery template library based on es6 tagged template",
  "main": "index.js",
  "dependencies": {
    "jquery": "^3.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "browserify-shim": "^3.8.14",
    "grunt": "^1.0.4",
    "grunt-babel": "^8.0.0",
    "grunt-browserify": "^5.3.0",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-uglify": "^4.0.1",
    "load-grunt-tasks": "^5.1.0",
    "rimraf": "^3.0.0"
  },
  "browserify-shim": {
    "jQuery": "global:jQuery"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf src-compiled",
    "build": "rimraf dist && rimraf src-compiled && babel src --out-dir src-compiled && browserify src-compiled/index.js -o dist/j6t.js && uglifyjs ./dist/j6t.js -o ./dist/j6t.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ironcodev/j6t.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ironcodev/j6t/issues"
  },
  "homepage": "https://github.com/ironcodev/j6t#readme"
}
